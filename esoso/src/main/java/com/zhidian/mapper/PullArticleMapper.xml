<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.zhidian.mapper.PullArticleMapper">


	<insert id="addArticle" parameterType="PullArticle" keyColumn="id" useGeneratedKeys="true" 
		keyProperty="id">
		insert into
		pullArticles(uuid,tags,title,contents,cssVersion,cssPath,website,startTime)
		values(#{uuid},#{tags},#{title},#{contents},#{cssVersion},#{cssPath},#{website},#{startTime})
	</insert>

	<select id="queryPullArticlesForPullArticleService01SimplePullArticle" parameterType="string" resultType="PullArticle">
		select * from pullArticles where uuid = #{uuid} and pullArticles.using = 1 and pullArticles.status = 0 group by startTime desc limit 1;
	</select>

</mapper>
